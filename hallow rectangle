#include <iostream>
using namespace std;
int main() 
{
    int n,m;
    cout<< "enter number";
    cin>>n;
    cout<< "enter number";
    cin>>m;
    for(int i=1;i<=n;i++)
    {
       for(int j=1;j<=m;j++)
       {
           if((j==1) || (i==1) || (j==m) || (i==n))
           {
               cout<<"*";
           }
           else
           {
               cout<<" ";
           }
       }
        cout<<endl;
       
    }

    return 0;
}
